ProfileList {
  a.profile {
    background-color: rgba(255, 255, 255, 0.74)
    border-radius: 5px
    transition: background-color 0.2s
    @svg connected {
      content: "<circle cx='6' stroke='none' fill='green' cy='6' r='5' />"
    }
    div.main {
      div.name {
        font-weight: bold
        font-size: 110%
        color: #636363
      }
    }
    :hover {
      background-color: rgba(255, 255, 255, 0.4)
    }
  }
}
